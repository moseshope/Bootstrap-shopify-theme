    "with-elements": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.7",
    "@shopify/themekit": "^1.1.9",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.30.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-livereload": "^4.0.2",
